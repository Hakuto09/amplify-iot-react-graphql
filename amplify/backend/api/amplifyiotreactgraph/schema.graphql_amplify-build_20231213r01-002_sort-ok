type Note
	@model
	@auth(rules: [ { allow: public } ] )
{
	id: ID! @primaryKey(sortKeyFields: ["date"])
	name: String
	description: String
	nickname: String
	date: String!
	send_cnt: Int
	magx: Float
	magy: Float
	magz: Float
	degree: Float
	distance: Float
	pres: Float
	temp: Float
	humi: Float
	postType: PostType!
}

enum PostType {
	OPEN
	SECRET
}
